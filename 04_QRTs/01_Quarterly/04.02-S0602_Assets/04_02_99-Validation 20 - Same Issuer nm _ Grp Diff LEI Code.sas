DATA CHECK_LEI_CD;
RETAIN CUSIP C0040 C0200 C0240 A31 A32 A33 A33GROUP;
KEEP CUSIP C0040 C0200 C0240  A31 A32 A33 A33GROUP;;
SET S0602_D1_IMT;
WHERE (C0200<>C0240 AND A31=A32);
RUN;

DATA CHECK_ISSUER_NM;
RETAIN CUSIP C0040 C0200 C0240 A31 A32 A33 A33GROUP;
KEEP CUSIP C0040 C0200 C0240  A31 A32 A33 A33GROUP;;
SET S0602_D1_IMT;
WHERE (C0200=C0240 AND A31<>A32);
RUN;

TITLE underlin=2 "Issuer name and Issuer Group name are diff , but thier LEI CD are same";
PROC PRINT DATA=CHECK_LEI_CD;
run;


TITLE underlin=2 "Issuer name and Issuer Group name are same , but thier LEI CD are diff";
PROC PRINT DATA=CHECK_ISSUER_NM;
run;

title;